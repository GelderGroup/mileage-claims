{
    "name": "mileage-claims",
    "version": "1.0.13",
    "scripts": {
        "start": "npm run clean && parcel src/index.html --open",
        "clean": "powershell -Command \"if (Test-Path dist) { Remove-Item dist -Recurse -Force }\"",
        "build": "npm run clean && parcel build src/index.html --no-source-maps && powershell -Command \"Copy-Item -Path staticwebapp.config.json -Destination dist\\staticwebapp.config.json\""
    },
    "type": "module",
    "dependencies": {
        "@picocss/pico": "^2.1.1",
        "ky": "^1.8.1",
        "redom-components": "^1.0.16"
    },
    "devDependencies": {
        "parcel": "^2.16.0",
        "redom": "^4.3.0"
    }
}