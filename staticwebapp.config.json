{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/images/*",
            "/css/*",
            "/*.css",
            "/*.js",
            "/favicon.ico",
            "/lib/*",
            "/assets/*"
        ]
    },
    "responseOverrides": {
        "401": {
            "redirect": "/.auth/login/aad",
            "statusCode": 302
        }
    },
    "routes": [
        {
            "route": "/api/*",
            "allowedRoles": [
                "authenticated"
            ]
        }
    ],
    "auth": {
        "identityProviders": {
            "azureActiveDirectory": {
                "registration": {
                    "openIdIssuer": "https://login.microsoftonline.com/009c4932-1b42-4c0f-ae49-eed8b8c9a26f/v2.0",
                    "clientIdSettingName": "AZURE_CLIENT_ID",
                    "clientSecretSettingName": "AZURE_CLIENT_SECRET"
                }
            }
        }
    }
}