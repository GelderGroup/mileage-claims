{
    "navigationFallback": {
        "rewrite": "/app.html",
        "exclude": [
            "/api/*",
            "/.auth/*",
            "/images/*",
            "/css/*"
        ]
    },
    "routes": [
        {
            "route": "/api/*",
            "allowedRoles": [
                "authenticated"
            ]
        }
    ]
}